{
  "name": "@rxdi/deploy",
  "version": "0.5.7",
  "description": "Package prepared to deploy easy decentralized rxdi package ",
  "main": "index.js",
  "scripts": {
    "postpublish": "git push -f ssh://git@gitlab.youvolio.com:522/rxdi/deploy.git && git push --tags && git push ssh://git@gitlab.youvolio.com:522/rxdi/deploy.git --tags",
    "start": "gapi start --local --parcel",
    "build:docker": "docker build -t rxdi/deployer .",
    "start:docker": "docker-compose up -d",
    "build": "gapi build",
    "deploy": "docker run -v /proba/:/usr/src/app/proba --name rxdi-deployer rxdi/deployer ./proba/index.ts || true && npm run stop",
    "stop": "docker rm -f rxdi-deployer",
    "tsc": "./node_modules/.bin/tsc",
    "test": "jest",
    "lint": "echo lint",
    "pretest": "npm run lint"
  },
  "bin": {
    "rxdi-deploy": "./dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxdi/deploy"
  },
  "author": "Kristiyan Tachev",
  "license": "MIT",
  "dependencies": {
    "@gapi/core": "^1.5.9",
    "@gapi/ipfs": "^1.5.7",
    "@gapi/ipfs-daemon": "^1.5.9",
    "@rxdi/dts-merge": "^0.5.6",
    "flatten": "^1.0.2",
    "go-ipfs": "git+https://github.com/Stradivario/npm-go-ipfs.git",
    "graphql-request": "^1.8.2",
    "nedb": "^1.8.0",
    "ncp": "2.0.0",
    "parcel-bundler": "1.12.3",
    "terminal-table": "0.0.12",
    "typescript": "3.4.1"
  },
  "devDependencies": {
    "@types/hapi": "^17.0.19",
    "@types/nedb": "^1.8.6",
    "@types/node": "10.10.2",
    "parcel-plugin-shebang": "^1.2.3"
  },
  "types": "./dist/main.d.ts",
  "module": "./dist/main.js",
  "typings": "./dist/main.d.ts"
}
